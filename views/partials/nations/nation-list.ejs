<% if (nations.length> 0) { %>
   <% nations.forEach(nation=> { %>

      <!-- <a class="panel-block pt-5 pb-5" href="/nations/<%= nation.id %>">
         
      </a> -->

      <a href="/nations/<%= nation.id %>" class="column is-one-fifth">
         <div class="card">
            <div class="card-image">
               <figure class="image is-3by2">
                  <img src="<%= nation.imageUrl %>" alt="<%= nation.name %>" crossorigin="anonymous">
               </figure>

            </div>
         </div>
      </a>
      <% }) %>
         <% } %>